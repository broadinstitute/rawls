{{with $environment := env "ENVIRONMENT"}}
{{$keyname := printf "secret/dsde/%s/rawls/rawls-compose.yaml" $environment}}
{{with vault $keyname}}

app:
  image: {{.Data.app_image}}
  {{.Data.app_dns}}
  {{.Data.app_volumes}}
  {{.Data.app_ports}}
  {{.Data.log_driver}}
  restart: always
proxy:
  extends:
    file: proxy-compose.yaml
    service: proxy
  links:
    - app:app

{{end}}
{{end}}
